{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "IAROS Event Schema",
  "type": "object",
  "properties": {
    "event_id": {
      "type": "string"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "service": {
      "type": "string"
    },
    "event_type": {
      "type": "string"
    },
    "payload": {
      "type": "object"
    }
  },
  "required": ["event_id", "timestamp", "service", "event_type", "payload"]
}
